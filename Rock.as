package  {	import flash.display.Sprite;	import flash.events.Event;	public class Rock extends Sprite{		private var _body:Sprite;		private var _main:Main;		var hitHead:Boolean = false;				public function Rock(body:Sprite, main:Main) {			this._body = body;			this._main = main;			addChild(body);			addEventListener(Event.ENTER_FRAME, update);		}		private function update(evt:Event):void{						if(hitTestObject(_main.snake.head)){				_main.speed = 5;				hitHead = true;			}if(hitHead == true && !hitTestObject(_main.snake.head)){				_main.speed = _main.maxSpeed;				hitHead = false;				trace(_main.snake.nose.x)				trace(this.x - _main.scene.x)			}					}	}	}