package {	import flash.display.Sprite;	import flash.events.Event;	import flash.events.MouseEvent;	import flash.geom.Point;	public class Tile extends Sprite {		private var _main:Main;		protected var edge:Boolean = true;		public function Tile(main:Main, x:int, y:int) {			this.x = x;			this.y = y;			this._main = main;			var rock:Rock = new Rock(new Rock1(), main);			rock.x = Math.random() * 800 + 100;			rock.y = Math.random() * 800 + 100;			addChild(rock);			addEventListener(Event.ENTER_FRAME, update);		}		private function update(evt:Event):void {			if (hitTestObject(_main.snake.head) && edge) {				edge = false;				_main.populateNeighbors(this);				}		}	}}